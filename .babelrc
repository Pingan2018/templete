{
  "presets": [
    [
        "@babel/preset-env",
        {
            "useBuiltIns": "entry"
        }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
      [
          "@babel/plugin-proposal-decorators",
          {
              "legacy": true
          }
      ],
      [
          "@babel/plugin-proposal-class-properties",
          {
              "loose": true
          }
      ],
      [
          "babel-plugin-import",
          {
              "libraryName": "antd",
              "libraryDirectory": "es",
              "style": true
          }
      ],
      "@babel/plugin-transform-runtime"
  ]
}