{
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": 2,
                "useBuiltIns": "usage"
            }
        ],
        "@babel/preset-react"
        // [
        //     "@babel/preset-typescript",
        //     {
        //         "allExtensions": true
        //     }
        // ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        [
            "babel-plugin-import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css"
            }
        ],
        "@babel/plugin-transform-runtime"
    ]
}